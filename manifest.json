{
	"manifest_version": 1,
	"name": "SHC Helper",
	"version": "0.1",
	"permissions": [
		"cookies",
		"tabs",
		"notifications",
	],
	"content_scripts": [
		{
			"matches": ["http://*.sears.com/*",
						"http://*.kmart.com/*",
						"*://jira.intra.sears.com/*"
			],
			"js": ["shcHelper.js"]
		}
	]
}